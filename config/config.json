                "Parity": "none",
                "Timeout": 2.0,
                "SoftwareFlowControl": false,
                "HardwareFlowControl": false
            }
        }
    },
    "Sensors":
    {
        "ADAM-4015":
        {
            "Description": "ADAM-4015 I/O module.",
            "Type": "I/O",
            "Observations":
            {
                "Main":
                [
                    {
                        "Name": "get_values",
                        "Description": "Get temperature values.",
                        "Receivers": ["PreProcessor", "FileExporter"],
                        "Enabled": true,
                        "Onetime": false,
                        "AwaitTime": 0.8,
                        "SleepTime": 10.0,
                        "Queries":
                        [
                            {
                                "ID": "Pt100",
                                "Request": "#01\r",
                                "ResponsePattern": ">[+-][\\d\\.]{6}[+-][\\d\\.]{6}[+-][\\d\\.]{6}[+-][\\d\\.]{6}([+-][\\d\\.]{6})([+-][\\d\\.]{6})",
                                "ResponseDelimiter": "\r",
                                "ResponseDescriptions": ["Temperature", "Temperature"],
                                "ResponseTypes": ["Float", "Float"],
                                "ResponseUnits": ["C", "C" ]
                            }
                        ]
                    }
                ]
            }
        },
        "DFT-485":
        {
            "Description": "Reinhardt DFT 485 weather station.",
            "Type": "Meteorological",
            "Observations":
            {
                "Main":
                [
                    {
                        "Name": "get_values",
                        "Description": "Get temperature 1, temperature 2, pressure, and humidity .",
                        "Receivers": ["PreProcessor", "FileExporter"],
                        "Enabled": true,
                        "Onetime": false,
                        "AwaitTime": 1.0,
                        "SleepTime": 5.0,
                        "Queries":
                        [
                            {
                                "ID": "Meteo",
                                "Request": "?U\r\n",
                                "ResponsePattern": "TD([\\d\\.]+), TE([\\d\\.]+), DR([\\d\\.]+), FE([\\d\\.]+)",
                                "ResponseDelimiter": ", \r\n",
                                "ResponseDescriptions": ["Temperature", "Temperature", "Pressure", "Humidity"],
                                "ResponseTypes": ["Float", "Float", "Float", "Float"],
                                "ResponseUnits": ["deg C", "deg C", "hPa", "%"]
                            }
                        ]
                    }
                ]
            }
        },
        "Leica_TS16":
        {
            "Description": "Leica TS16 totalstation",
            "Type": "Totalstation",
            "Observations":
            {
                "Init":
                [
                    {
                        "Name": "get_sensor_id",
                        "Description": "Get instrument identification code.",
                        "Receivers": ["PreProcessor"],
                        "Enabled": true,
                        "Onetime": true,
                        "AwaitTime": 1.0,
                        "SleepTime": 1.0,
                        "Queries":
                        [
                            {
                                "ID": null,
                                "Request": "%R1Q,5003:\r\n",
                                "ResponsePattern": "R1P,0,0:0,(\\d+)",
                                "ResponseDelimiter": "\r",
                                "ResponseDescriptions": "SensorID",
                                "ResponseTypes": "String",
                                "ResponseUnits": "none"
                            }
                        ]
                    },
                    {
                        "Name": "get_sensor_name",
                        "Description": "Get instrument name.",
                        "Receivers": ["PreProcessor"],
                        "Enabled": true,
                        "Onetime": true,
                        "AwaitTime": 0.8,
                        "SleepTime": 1.0,
                        "Queries":
                        [
                            {
                                "ID": null,
                                "Request": "%R1Q,5004:\r\n",
                                "ResponsePattern": "R1P,0,0:0,\"([\\w\\d\\s\\.]+)",
                                "ResponseDelimiter": "\r",
                                "ResponseDescriptions": "SensorName",
                                "ResponseTypes": "String",
                                "ResponseUnits": "none"
                            }
                        ]
                    }
                ],
                "P1":
                [
                    {
                        "Name": "set_direction",
                        "Description": "Set the horizontal and vertical direction.",
                        "Receivers": ["PreProcessor"],
                        "Enabled": true,
                        "Onetime": false,
                        "AwaitTime": 0.5,
                        "SleepTime": 2.0,
                        "Queries":
