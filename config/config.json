    {
        "Name": "Development Project 1",
        "Meta": "Project for testing only."
    },
    "Ports":
    {
       "Serial":
       {
           "ttyp0":
            {
                "Port": "/dev/ttyp0",
                "BaudRate": 115200,
                "ByteSize": 8,
                "StopBits": 1,
                "Parity": "none",
                "Timeout": 2.0,
                "SoftwareFlowControl": false,
                "HardwareFlowControl": false
            },
           "ttyp1":
            {
                "Port": "/dev/ttyp1",
                "BaudRate": 115200,
                "ByteSize": 8,
