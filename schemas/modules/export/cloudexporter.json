        "api",
        "user",
        "password",
        "authMethod",
        "db",
        "storage"
    ],
    "properties": {
        "host": {
            "$id": "/properties/host",
            "type": "string"
        },
        "api": {
            "$id": "/properties/api",
            "type": "string"
        },
        "user": {
            "$id": "/properties/user",
            "type": "string"
        },
        "password": {
            "$id": "/properties/password",
            "type": "string"
        },
        "authMethod": {
            "$id": "/properties/authMethod",
            "type": "string"
        },
        "db": {
            "$id": "/properties/db",
            "type": "string"
        },
        "storage": {
