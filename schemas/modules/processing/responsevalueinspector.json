{
    "$schema": "http://json-schema.org/draft-06/schema#",
    "id": "schemes/modules/processing/responsevalueinspector.json",
    "properties": {
        "observations": {
            "id": "/properties/observations",
            "patternProperties": {
                "^[a-zA-Z0-9]+$": {
                    "id": "/properties/observations/properties/observationName",
                    "patternProperties": {
                        "^[a-zA-Z0-9]+$": {
                            "id": "/properties/observations/properties/observationName/properties/responseName",
                            "properties": {
                                "max": {
                                    "id": "/properties/observations/properties/observationName/properties/responseName/properties/max",
                                    "type": "number"
                                },
                                "min": {
                                    "id": "/properties/observations/properties/observationName/properties/responseName/properties/min",
                                    "type": "number"
                                }
                            },
                            "type": "object"
                        }
                    },
                    "type": "object"
                }
            },
            "type": "object"
        }
    },
    "type": "object"
}
